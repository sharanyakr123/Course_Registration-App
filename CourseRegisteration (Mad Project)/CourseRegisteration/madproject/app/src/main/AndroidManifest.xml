<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.CourseRegisteration">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Library"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.CourseRegisteration.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.CourseRegisteration.ViewLibraryActivity"
            android:label="Book List"
            android:parentActivityName="com.example.CourseRegisteration.MainActivity"/>
        <activity
            android:name="com.example.CourseRegisteration.MainActivity"
            android:exported="true"
            android:label="Add Book">

        </activity>
    </application>

</manifest>